// Place your key bindings in this file to override the defaultsauto[]
[
  {
    "key": "ctrl+alt+w",
    "command": "editor.emmet.action.wrapWithAbbreviation"
  },
  {
    "key": "ctrl+alt+.",
    "command": "workbench.action.toggleActivityBarVisibility"
  },
  {
    "key": "ctrl+r f",
    "command": "workbench.action.remote.close"
  },
  {
    "key": "ctrl+alt+z",
    "command": "workbench.action.closeActiveEditor"
  },
  {
    "key": "ctrl+f4",
    "command": "-workbench.action.closeActiveEditor"
  },
  {
    "key": "alt+.",
    "command": "workbench.action.terminal.toggleTerminal",
    "when": "terminal.active"
  },
  {
    "key": "ctrl+up", // whatever keybinding you want
    "command": "cursorMove",
    "args": {
      "to": "up",
      "by": "line",
      "value": 10 // change this if you want
    },
    "when": "editorTextFocus"
  },
  {
    "key": "ctrl+down", // whatever keybinding you want
    "command": "cursorMove",
    "args": {
      "to": "down",
      "by": "line",
      "value": 10 // change
    },
    "when": "editorTextFocus"
  },
  {
    "key": "ctrl+alt+t",
    "command": "workbench.action.reopenClosedEditor"
  },
  {
    "key": "ctrl+alt+p",
    "command": "~remote.forwardedPorts.focus"
  },
  {
    "key": "alt+/",
    "command": "remote-wsl.explorer.openDistroInCurrentWindow"
  },
  {
    "key": "ctrl+shift+u",
    "command": "-workbench.action.output.toggleOutput",
    "when": "workbench.panel.output.active"
  },
  {
    "key": "ctrl+shift+u",
    "command": "editor.action.transformToUppercase"
  },
  {
    "key": "ctrl+shift+l",
    "command": "editor.action.transformToLowercase"
  },
  {
    "key": "ctrl+alt+c",
    "command": "editor.action.transformToCamelcase"
  }
]
